<?xml version="1.0" encoding="UTF-8"?>
<openapoc>
  <form id="FORM_RESEARCHSELECT">
    <style minwidth="640" minheight="480">
      <position x="centre" y="centre"/>
      <size width="640" height="480"/>
      <graphic>
        <image>xcom3/ufodata/researc2.pcx</image>
        <position x="0" y="0"/>
        <size width="640" height="480"/>
      </graphic>
      <label text="SELECT PROJECT" id="TEXT_TITLE">
        <position x="20" y="0"/>
        <size width="600" height="36"/>
        <alignment horizontal="centre" vertical="centre"/>
        <font>bigfont</font>
      </label>
      <label id="TEXT_FUNDS">
        <position x="572" y="11"/>
        <size width="60" height="10"/>
        <alignment horizontal="left" vertical="centre"/>
        <font>smalfont</font>
      </label>
      <graphicbutton id="BUTTON_OK">
        <position x="602" y="443"/>
        <size width="35" height="34"/>
        <image/>
        <imagedepressed>BUTTON_OK_DEPRESSED</imagedepressed>
      </graphicbutton>
      <scroll id="LIST_SCROLL">
        <position x="567" y="71"/>
        <size width="26" height="259"/>
      </scroll>
      <graphicbutton id="LIST_SCROLL_UP" scrollprev="LIST_SCROLL">
        <tooltip text="Scroll Up" font="smallset"/>
        <position x="569" y="47"/>
        <size width="22" height="21"/>
        <image/>
        <imagedepressed>BUTTON_SCROLL_UP_DEPRESSED</imagedepressed>
      </graphicbutton>
      <graphicbutton id="LIST_SCROLL_DOWN" scrollnext="LIST_SCROLL">
        <tooltip text="Scroll Down" font="smallset"/>
        <position x="569" y="334"/>
        <size width="22" height="21"/>
        <image/>
        <imagedepressed>BUTTON_SCROLL_DOWN_DEPRESSED</imagedepressed>
      </graphicbutton>
      <listbox id="LIST" scrollbarid="LIST_SCROLL">
        <position x="17" y="82"/>
        <size height="266" width="542"/>
        <orientation>vertical</orientation>
        <hovercolour r="255" g="255" b="255" a="255"/>
        <autoselect>no</autoselect>
      </listbox>
      <label text="Project">
        <position x="23" y="63"/>
        <size height="15" width="200"/>
        <alignment horizontal="left" vertical="top"/>
        <font>smalfont</font>
      </label>
      <label text="Progress" id="TEXT_PROGRESS">
        <position x="251" y="63"/>
        <size height="15" width="80"/>
        <alignment horizontal="left" vertical="top"/>
        <font>smalfont</font>
      </label>
      <label text="Skill" id="TEXT_SKILL">
        <position x="345" y="63"/>
        <size height="15" width="50"/>
        <alignment horizontal="right" vertical="top"/>
        <font>smalfont</font>
      </label>
      <label text="Lab Size Needed">
        <position x="456" y="63"/>
        <size height="15" width="100"/>
        <alignment horizontal="left" vertical="top"/>
        <font>smalfont</font>
      </label>
      <label id="TEXT_SELECTED_TITLE">
        <position x="23" y="365"/>
        <size height="15" width="550"/>
        <alignment horizontal="left" vertical="centre"/>
        <font>smalfont</font>
      </label>
      <label id="TEXT_SELECTED_DESCRIPTION">
        <position x="120" y="386"/>
        <size height="80" width="450"/>
        <alignment horizontal="left" vertical="top"/>
        <font>smalfont</font>
      </label>
      <graphic id="GRAPHIC_SELECTED">
        <position x="23" y="383"/>
        <size height="80" width="96"/>
        <font>smalfont</font>
      </graphic>
    </style>
  </form>
</openapoc>
